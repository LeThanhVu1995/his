SERVICE_NAME=master-data-service
SERVICE_PORT=8011
RUST_LOG=info,actix_web=info

# Database
DATABASE_URL=postgres://postgres:postgres@localhost:5432/master_data

# IAM/Keycloak or IAM-service
# Nếu dùng Keycloak trực tiếp:
KEYCLOAK_ISSUER=https://auth.local/realms/myapp
KEYCLOAK_JWKS=https://auth.local/realms/myapp/protocol/openid-connect/certs
AUTH_AUDIENCE=master-api

# Nếu có IAM-service đứng trước (khuyến nghị):
IAM_SERVICE_BASE_URL=http://localhost:8001/api/v1/iam
IAM_SERVICE_TOKEN=changeme-admin-token     # token để đăng ký permission definitions

# Kafka (tuỳ chọn)
KAFKA_BROKERS=localhost:9092
KAFKA_CLIENT_ID=master-data-service

# OTEL (tuỳ chọn)
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317
